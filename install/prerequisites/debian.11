#!/bin/sh
extension_prerequisites(){
  apt-get update
  apt-get install -y default-jre 
  apt-get install -y unzip 
  apt-get install -y libutfcpp-dev 
  apt-get install -y cmake 
  apt-get install -y curl
  apt-get install -y git
}
core_prerequisites(){
  apt-get update
  apt-get install -y apt-utils 
  apt-get install -y build-essential flex libxml2-dev bison libreadline-dev zlib1g-dev
  apt-get install -y uuid-dev pkg-config libossp-uuid-dev libssl-dev icu-devtools
  apt-get install -y wget
}
